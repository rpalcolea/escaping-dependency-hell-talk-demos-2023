plugins {
    id 'java-library'
}

repositories {
    mavenCentral()
    maven {
        url "https://repo.spring.io/release"
    }
    maven {
        url "https://repository.jboss.org/maven2"
    }
    maven {
        url = 'localRepo'
    }
}
dependencies {
    implementation 'com.google.guava:guava:29.0-jre'
    implementation 'info.perezalcolea:bestlibrary:latest.release'
}
